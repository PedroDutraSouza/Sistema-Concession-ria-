procedimento calculoaluguelmoto ()
inicio
m1 <- (dias*39)// dias x 39 reais - moto
m2 <- (182*36) * semestre// semestre x 36 reais - moto
m3 <- (365*33) * ano// ano x 33 reais - moto

fimprocedimento


